{
  "problemMatcher": [
    {
      "owner": "rustc",
      "pattern": [
        {
          "regexp": "^(warning|error)(?:\\s*\\[(E\\d{4})\\])?:\\s(.*)$",
          "severity": 1,
          "code": 2,
          "message": 3
        },
        {
          "regexp": "^\\s*-->\\s(.+):(\\d+):(\\d+)$",
          "file": 1,
          "line": 2,
          "column": 3
        }
      ],
      "loop": true
    }
  ]
}
